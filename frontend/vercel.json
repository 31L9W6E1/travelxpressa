{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://travelxpressa-backend-production.up.railway.app/api/$1"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "https://travelxpressa-backend-production.up.railway.app/uploads/$1"
    },
    { "source": "/(.*)", "destination": "/" }
  ]
}
